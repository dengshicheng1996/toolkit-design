{
  "private": true,
  "name": "toolkit-design",
  "version": "1.0.0",
  "license": "MIT",
  "author": "邓世城",
  "homepage": "https://github.com/dengshicheng1996/toolkit-design.git#readme",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "changelog": {
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run tsc && npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "array-tree-filter": "^2.1.0",
    "@ant-design/mobile-icons": "^3.0.0-alpha.8",
    "@ant-design/mobile-languages": "^3.0.0-alpha.8",
    "@ant-design/mobile-touchable": "^3.0.0-alpha.8",
    "rc-collapse": "^3.1.1",
    "rc-field-form": "^1.21.1",
    "rc-swipeout": "^2.0.11",
    "react": "^16.12.0",
    "react-swipeable-views": "^0.14.0",
    "rmc-cascader": "^5.0.3",
    "rmc-dialog": "^1.1.1",
    "rmc-feedback": "^2.0.0",
    "rmc-picker": "^5.0.10",
    "rmc-tooltip": "^1.0.1",
    "swiper": "^6.7.1"
  },
  "devDependencies": {
    "lerna": "^4.0.0",
    "@umijs/test": "^3.0.5",
    "babel-plugin-version": "^0.2.3",
    "dumi": "^1.0.14",
    "dumi-theme-mobile": "^1.1.3",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
